{
  "name": "cognitive-vite",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && vite",
    "deploy": "npm run build && gh-pages -d dist",
    "build": "npm run clean && vite build --mode deploy",
    "serve": "npm run clean && vite build && vite preview --port 8080",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "=1.0.0-next.30",
    "gh-pages": "=3.2.3",
    "prettier": "=2.4.1",
    "prettier-plugin-svelte": "=2.5.0",
    "rimraf": "=3.0.2",
    "svelte": "=3.44.2",
    "svelte-preprocess": "=4.9.8",
    "vite": "=2.6.14",
    "mdsvex": "=0.9.8",
    "sass": "=1.43.4",
    "svelte-spa-router": "=3.2.0"
  }
}
