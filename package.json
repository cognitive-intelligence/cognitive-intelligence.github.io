{
  "name": "cognitive-vite",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run clean && vite",
    "deploy": "npm run build && gh-pages -d dist",
    "build": "npm run clean && vite build --mode deploy",
    "serve": "npm run clean && vite build && vite preview --port 8080",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "=1.0.0-next.41",
    "gh-pages": "=3.2.3",
    "prettier": "=2.6.2",
    "prettier-plugin-svelte": "=2.7.0",
    "rimraf": "=3.0.2",
    "svelte": "=3.47.0",
    "svelte-preprocess": "=4.10.6",
    "vite": "=2.9.5",
    "mdsvex": "=0.10.5",
    "sass": "=1.50.0",
    "svelte-spa-router": "=3.2.0"
  }
}
